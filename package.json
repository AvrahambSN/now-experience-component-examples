        "@servicenow/cli-component-archetype": "quebec",
        "@servicenow/now-avatar": "quebec",
        "@servicenow/now-button": "quebec",
        "@servicenow/now-heading": "quebec",
        "@servicenow/now-icon": "quebec",
        "@servicenow/now-label-value": "quebec",
        "@servicenow/now-loader": "quebec",
        "@servicenow/now-toggle": "quebec",
        "@servicenow/sass-kit": "quebec",
        "@servicenow/sass-theme": "quebec",
        "@servicenow/ui-core": "quebec",
        "@servicenow/ui-effect-http": "quebec",
        "@servicenow/ui-renderer-snabbdom": "quebec"
